keywords = {Gas and ethanol permeability,Klinkenberg effect,Limestone,Mineralogical composition,Poro-elastic properties},
number = {6},
pages = {915--925},
title = {{Determination of the main hydraulic and poro-elastic properties of a limestone from Bourgogne, France}},
volume = {41},
