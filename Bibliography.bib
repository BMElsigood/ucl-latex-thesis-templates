keywords = {10.1002/2017JB014315 and poroelasticity,earthquake mechanics,elastic anisotropy,rock physics},
number = {B4},
pages = {1--15},
title = {{Anisotropic Poroelasticity in a Rock With Cracks}},
volume = {122},
year = {2017}
}
